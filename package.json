  "devDependencies": {
    "chai": "^2.0.0",
    "jshint": "^2.5.5",
    "lintspaces-cli": "0.0.4",
    "mocha": "^1.21.4",
    "mocha-phantomjs": "git+https://github.com/georgecrawford/mocha-phantomjs.git#javascript",
    "phantomjs": "^1.9.15"
